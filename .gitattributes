# Set the default behavior, in case people don't have core.autocrlf set.
* text=auto

# Force text files to have LF
*.c text eol=lf
*.cpp text eol=lf
*.h text eol=lf
*.java text eol=lf
*.prefs text eol=lf
*.txt text eol=lf
*.adq text eol=lf
*.eqp text eol=lf
*.skl text eol=lf
*.spl text eol=lf
*.gcs text eol=lf
*.gct text eol=lf
*.html text eol=lf
*.xml text eol=lf

# Denote all files that are truly binary and should not be modified.
*.png binary
*.jpg binary
*.psd binary
*.zip binary
*.jar binary
